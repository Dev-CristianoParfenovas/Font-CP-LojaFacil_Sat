unit untInstrucoesCaixa;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls;

type
  TfrmInstrucoesCaixa = class(TForm)
    lbl_F1: TLabel;
    lbl_F2: TLabel;
    lbl_F3: TLabel;
    lbl_F4: TLabel;
    lbl_F7: TLabel;
    lbl_f8: TLabel;
    lbl_f9: TLabel;
    lbl_VendaMetro: TLabel;
    lbl_F11: TLabel;
    lbl_Ctrlc: TLabel;
    lbl_F5: TLabel;
    lbl_F6: TLabel;
    lbl_Ctrn: TLabel;
    lbl_Ctrl: TLabel;
    lbl_CTRLX: TLabel;
    lbl_CTRLZ: TLabel;
    lbl_CTRLP: TLabel;
    lbl_Aviso: TLabel;
    Label1: TLabel;
    Label2: TLabel;
    Label3: TLabel;
    Label4: TLabel;
    Label5: TLabel;
    procedure FormKeyDown(Sender: TObject; var Key: Word;
      Shift: TShiftState);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmInstrucoesCaixa: TfrmInstrucoesCaixa;

implementation

{$R *.dfm}

procedure TfrmInstrucoesCaixa.FormKeyDown(Sender: TObject; var Key: Word;
  Shift: TShiftState);
begin

  case key of

    VK_F2 :begin

      frmInstrucoesCaixa.Close;

    end;//begin

  end;//case

end;

end.
